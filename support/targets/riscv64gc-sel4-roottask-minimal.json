{
  "arch": "riscv64",
  "code-model": "medium",
  "cpu": "generic-rv64",
  "crt-objects-fallback": "false",
  "data-layout": "e-m:e-p:64:64-i64:64-i128:128-n32:64-S128",
  "emit-debug-gdb-scripts": false,
  "exe-suffix": ".elf",
  "features": "+m,+a,+f,+d,+c,+zicsr,+zifencei",
  "linker": "rust-lld",
  "linker-flavor": "gnu-lld",
  "llvm-abiname": "lp64d",
  "llvm-target": "riscv64",
  "max-atomic-width": 64,
  "metadata": {
    "description": null,
    "host_tools": null,
    "std": null,
    "tier": null
  },
  "panic-strategy": "abort",
  "pre-link-args": {
    "gnu-lld": [
      "--no-rosegment"
    ]
  },
  "relocation-model": "static",
  "supported-sanitizers": [
    "shadow-call-stack",
    "kernel-address"
  ],
  "target-pointer-width": 64
}
